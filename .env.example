# Database connection (for local development)
MONGODB_URI=mongodb+srv://[username]:[password]@[cluster].mongodb.net/[database]?retryWrites=true&w=majority

# Server configuration (for local development)
PORT=3001

# Authentication credentials (IMPORTANT - CHANGE THESE!)
ADMIN_USERNAME=admin
ADMIN_PASSWORD=CHANGE_THIS_PASSWORD
JWT_SECRET=your_jwt_secret

# KoboToolbox API Configuration (needed for Vercel deployment)
KOBO_API_URL=https://kf.kobotoolbox.org/api/v2
KOBO_API_TOKEN=your_kobo_api_token
KOBO_ASSET_ID=your_kobo_form_asset_id

# Production API URL (only if you're not using Vercel API routes)
VITE_API_URL=https://your-api-server.com/api 